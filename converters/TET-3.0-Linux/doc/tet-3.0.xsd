									<xs:attribute name="number" use="required">
										<xs:simpleType>
											<xs:restriction base="xs:string"/>
										</xs:simpleType>
									</xs:attribute>
									<xs:attribute name="height" type="xs:double" use="required"/>
									<xs:attribute name="width" type="xs:double" use="required"/>
								</xs:complexType>
							</xs:element>
							<xs:element ref="tet:Resources" minOccurs="0"/>
						</xs:sequence>
					</xs:complexType>
					<xs:key name="FontKey">
						<xs:annotation>
							<xs:documentation>Within the Pages element the Font elements are identified uniquely by their @id attribute.</xs:documentation>
						</xs:annotation>
						<xs:selector xpath="tet:Resources/tet:Fonts/tet:Font"/>
						<xs:field xpath="@id"/>
					</xs:key>
					<xs:keyref name="FontRef" refer="tet:FontKey">
						<xs:annotation>
							<xs:documentation>All Glyph elements within the Pages element refer to an existing Font element with their @font attribute.</xs:documentation>
						</xs:annotation>
						<xs:selector xpath=".//tet:Glyph"/>
						<xs:field xpath="@font"/>
					</xs:keyref>
					<xs:key name="ColorSpaceKey">
						<xs:annotation>
							<xs:documentation>Within the Pages element the ColorSpace elements are identified uniquely by their @id attribute.</xs:documentation>
						</xs:annotation>
