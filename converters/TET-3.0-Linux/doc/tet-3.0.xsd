						<xs:field xpath="@colorspace"/>
					</xs:keyref>
					<xs:keyref name="BaseColorSpaceRef" refer="tet:ColorSpaceKey">
						<xs:annotation>
							<xs:documentation>A ColorSpace can refer to an existing base ColorSpace.</xs:documentation>
						</xs:annotation>
						<xs:selector xpath="tet:Resources/tet:ColorSpaces/tet:ColorSpace"/>
						<xs:field xpath="@base"/>
					</xs:keyref>
					<xs:keyref name="AlternateColorSpaceRef" refer="tet:ColorSpaceKey">
						<xs:annotation>
							<xs:documentation>A ColorSpace can refer to an existing alternate ColorSpace.</xs:documentation>
