												</xs:restriction>
											</xs:simpleType>
										</xs:attribute>
										<xs:attribute name="pagenumber" type="xs:positiveInteger" use="optional"/>
									</xs:complexType>
								</xs:element>
							</xs:sequence>
						</xs:choice>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="filename" use="required">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="pageCount" type="xs:positiveInteger" use="required"/>
			<xs:attribute name="filesize" type="xs:positiveInteger" use="required"/>
			<xs:attribute name="linearized" use="required">
				<xs:simpleType>
					<xs:restriction base="xs:boolean"/>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="pdfVersion" use="required">
				<xs:simpleType>
					<xs:restriction base="xs:string"/>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="pdfa" use="optional" default="none">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="none"/>
						<xs:enumeration value="PDF/A-1a:2005"/>
						<xs:enumeration value="PDF/A-1b:2005"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="pdfx" use="optional" default="none">
				<xs:simpleType>
					<xs:restriction base="xs:string"/>
				</xs:simpleType>
