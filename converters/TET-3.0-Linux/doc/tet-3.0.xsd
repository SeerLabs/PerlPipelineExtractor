						</xs:annotation>
						<xs:selector xpath="tet:Custom | tet:CustomBinary"/>
						<xs:field xpath="@key"/>
					</xs:key>
				</xs:element>
				<xs:element ref="tet:Metadata" minOccurs="0"/>
				<xs:element ref="tet:Options" minOccurs="0"/>
				<xs:element ref="tet:Exception" minOccurs="0"/>
				<xs:element name="Pages">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Page" maxOccurs="unbounded">
								<xs:complexType>
									<xs:sequence>
										<xs:element ref="tet:Options" minOccurs="0"/>
										<xs:element ref="tet:Content"/>
										<xs:element ref="tet:Exception" minOccurs="0"/>
