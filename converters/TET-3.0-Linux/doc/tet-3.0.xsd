				</xs:sequence>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="Resources">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="Fonts" minOccurs="0">
					<xs:complexType>
						<xs:choice>
							<xs:element ref="tet:Exception" minOccurs="0"/>
