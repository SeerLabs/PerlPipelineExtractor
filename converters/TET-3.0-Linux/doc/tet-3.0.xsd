						<xs:field xpath="@alternate"/>
					</xs:keyref>
					<xs:key name="ImageKey">
						<xs:annotation>
							<xs:documentation>Within the Pages element the Image elements are identified uniquely by their @id attribute.</xs:documentation>
						</xs:annotation>
						<xs:selector xpath="tet:Resources/tet:Images/tet:Image"/>
						<xs:field xpath="@id"/>
					</xs:key>
					<xs:keyref name="ImageRef" refer="tet:ImageKey">
						<xs:annotation>
							<xs:documentation>All PlacedImage elements within the Pages element refer to an existing Image element with their @image attribute.</xs:documentation>
						</xs:annotation>
						<xs:selector xpath=".//tet:PlacedImage"/>
						<xs:field xpath="@image"/>
					</xs:keyref>
					<xs:keyref name="ImageMaskRef" refer="tet:ImageKey">
						<xs:annotation>
							<xs:documentation>An Image can refer to another Image by its @mask attribute.</xs:documentation>
						</xs:annotation>
						<xs:selector xpath="tet:Resources/tet:Images/tet:Image"/>
						<xs:field xpath="@mask"/>
					</xs:keyref>
				</xs:element>
				<xs:element name="Attachments" minOccurs="0">
					<xs:complexType>
						<xs:choice>
							<xs:element ref="tet:Exception" minOccurs="0"/>
							<xs:sequence minOccurs="0">
								<xs:element name="Attachment" maxOccurs="unbounded">
									<xs:complexType>
										<xs:choice minOccurs="0">
											<xs:element ref="tet:Exception" minOccurs="0"/>
