<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:tet="http://www.pdflib.com/XML/TET3/TET-3.0" targetNamespace="http://www.pdflib.com/XML/TET3/TET-3.0" elementFormDefault="qualified" version="3.0">
	<xs:element name="ColorSpace">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="tet:Exception" minOccurs="0"/>
				<xs:element name="Colorant" minOccurs="0" maxOccurs="unbounded">
					<xs:complexType>
						<xs:attribute name="name" type="xs:string" use="required"/>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="alternate" type="tet:colorSpaceId" use="optional">
				<xs:annotation>
					<xs:documentation>Only for name=Separation and DeviceN.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="base" type="tet:colorSpaceId" use="optional"/>
			<xs:attribute name="components" type="xs:positiveInteger" use="required"/>
			<xs:attribute name="id" type="tet:colorSpaceId" use="required"/>
			<xs:attribute name="name" use="required">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="CalGray"/>
						<xs:enumeration value="CalRGB"/>
						<xs:enumeration value="DeviceCMYK"/>
						<xs:enumeration value="DeviceGray"/>
						<xs:enumeration value="DeviceN"/>
						<xs:enumeration value="DeviceRGB"/>
						<xs:enumeration value="ICCBased"/>
						<xs:enumeration value="Indexed"/>
						<xs:enumeration value="Lab"/>
						<xs:enumeration value="Separation"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="Content">
		<xs:complexType mixed="false">
			<xs:group ref="tet:ContentGroup" minOccurs="0" maxOccurs="unbounded"/>
			<xs:attribute name="granularity" use="required">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="glyph"/>
						<xs:enumeration value="line"/>
						<xs:enumeration value="page"/>
						<xs:enumeration value="word"/>
						<xs:enumeration value="wordplus"/>
						<xs:enumeration value="zone"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="font" type="xs:boolean" use="required"/>
			<xs:attribute name="geometry" type="xs:boolean" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:group name="ContentGroup">
		<xs:choice>
			<xs:element name="Word" maxOccurs="unbounded">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Text" type="tet:text"/>
						<xs:element name="Box" maxOccurs="unbounded">
							<xs:complexType>
								<xs:sequence>
									<xs:element ref="tet:Glyph" minOccurs="0" maxOccurs="unbounded"/>
								</xs:sequence>
								<xs:attributeGroup ref="tet:BoxAttr"/>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="Line" maxOccurs="unbounded">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Text" type="tet:text"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="Para" maxOccurs="unbounded">
				<xs:complexType>
					<xs:choice>
						<xs:group ref="tet:ContentGroup" minOccurs="0" maxOccurs="unbounded"/>
						<xs:element name="Text" type="tet:text" minOccurs="0"/>
					</xs:choice>
				</xs:complexType>
			</xs:element>
			<xs:element name="Table" maxOccurs="unbounded">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Row" maxOccurs="unbounded">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="Cell" maxOccurs="unbounded">
										<xs:complexType>
											<xs:group ref="tet:ContentGroup" minOccurs="0" maxOccurs="unbounded"/>
											<xs:attribute name="colSpan" type="xs:positiveInteger" default="1"/>
										</xs:complexType>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="Zone" maxOccurs="unbounded">
				<xs:complexType>
					<xs:group ref="tet:ContentGroup" maxOccurs="unbounded"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="PlacedImage" maxOccurs="unbounded">
				<xs:complexType>
					<xs:attribute name="alpha" use="optional" default="0">
						<xs:simpleType>
							<xs:restriction base="xs:double">
								<xs:minExclusive value="-180"/>
								<xs:maxInclusive value="180"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:attribute>
					<xs:attribute name="beta" use="optional" default="0">
						<xs:simpleType>
							<xs:restriction base="xs:double">
								<xs:minExclusive value="-180"/>
								<xs:maxInclusive value="180"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:attribute>
					<xs:attribute name="height" use="required">
						<xs:simpleType>
							<xs:restriction base="xs:double">
								<xs:minInclusive value="0"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:attribute>
					<xs:attribute name="image" type="tet:imageId" use="required"/>
					<xs:attribute name="width" use="required">
						<xs:simpleType>
							<xs:restriction base="xs:double">
								<xs:minInclusive value="0"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:attribute>
					<xs:attribute name="x" use="required">
						<xs:simpleType>
							<xs:restriction base="xs:double"/>
						</xs:simpleType>
					</xs:attribute>
					<xs:attribute name="y" use="required">
						<xs:simpleType>
							<xs:restriction base="xs:double"/>
						</xs:simpleType>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
			<xs:element ref="tet:Glyph" maxOccurs="unbounded"/>
		</xs:choice>
	</xs:group>
	<xs:element name="Creation">
		<xs:complexType>
			<xs:attribute name="platform" use="required">
				<xs:simpleType>
					<xs:restriction base="xs:string"/>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="tetVersion" use="required">
				<xs:simpleType>
					<xs:restriction base="xs:string"/>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="date" type="xs:dateTime" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:group name="DocInfoGroup">
		<xs:sequence>
			<xs:element name="Author" type="xs:string" minOccurs="0"/>
			<xs:element name="CreationDate" type="xs:dateTime" minOccurs="0"/>
			<xs:element name="Creator" type="xs:string" minOccurs="0"/>
			<xs:element name="GTS_PDFXConformance" type="xs:string" minOccurs="0"/>
			<xs:element name="GTS_PDFXVersion" type="xs:string" minOccurs="0"/>
			<xs:element name="GTS_PPMLVDXConformance" type="xs:string" minOccurs="0"/>
			<xs:element name="GTS_PPMLVDXVersion" type="xs:string" minOccurs="0"/>
			<xs:element name="ISO_PDFEVersion" type="xs:string" minOccurs="0"/>
			<xs:element name="Keywords" type="xs:string" minOccurs="0"/>
			<xs:element name="ModDate" type="xs:dateTime" minOccurs="0"/>
			<xs:element name="Producer" type="xs:string" minOccurs="0"/>
			<xs:element name="Subject" type="xs:string" minOccurs="0"/>
			<xs:element name="Title" type="xs:string" minOccurs="0"/>
			<xs:element name="Trapped" type="xs:string" minOccurs="0"/>
			<xs:element name="Custom" minOccurs="0" maxOccurs="unbounded">
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="xs:string">
							<xs:attribute name="key" use="required">
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:minLength value="1"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:attribute>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="CustomBinary" minOccurs="0" maxOccurs="unbounded">
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="xs:hexBinary">
							<xs:attribute name="key" use="required">
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:minLength value="1"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:attribute>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:group>
	<xs:element name="Document">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="Encryption" minOccurs="0">
					<xs:complexType>
						<xs:attribute name="keylength" use="required">
							<xs:simpleType>
								<xs:restriction base="xs:integer">
									<xs:minInclusive value="40"/>
									<xs:maxInclusive value="256"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:attribute>
						<xs:attribute name="algorithm" use="required">
							<xs:simpleType>
								<xs:restriction base="xs:integer">
									<xs:minInclusive value="-1"/>
									<xs:maxInclusive value="7"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:attribute>
						<xs:attribute name="description" use="required">
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="Unknown encryption"/>
									<xs:enumeration value="No encryption"/>
									<xs:enumeration value="40-bit RC4 (Acrobat 2-4)"/>
									<xs:enumeration value="128-bit RC4 (Acrobat 5)"/>
									<xs:enumeration value="128-bit RC4 (Acrobat 6)"/>
									<xs:enumeration value="128-bit AES (Acrobat 7)"/>
									<xs:enumeration value="Public key on top of 128-bit RC4 (Acrobat 5)"/>
									<xs:enumeration value="Public key on top of 128-bit AES (Acrobat 7)"/>
									<xs:enumeration value="Adobe Policy Server (Acrobat 7)"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:attribute>
						<xs:attribute name="masterpassword" type="xs:boolean" use="required"/>
						<xs:attribute name="userpassword" type="xs:boolean" use="required"/>
						<xs:attribute name="noprint" type="xs:boolean" default="false"/>
						<xs:attribute name="nomodify" type="xs:boolean" default="false"/>
						<xs:attribute name="nocopy" type="xs:boolean" default="false"/>
						<xs:attribute name="noannots" type="xs:boolean" default="false"/>
						<xs:attribute name="noassemble" type="xs:boolean" default="false"/>
						<xs:attribute name="noforms" type="xs:boolean" default="false"/>
						<xs:attribute name="noaccessible" type="xs:boolean" default="false"/>
						<xs:attribute name="nohiresprint" type="xs:boolean" default="false"/>
						<xs:attribute name="plainmetadata" type="xs:boolean" default="false"/>
					</xs:complexType>
				</xs:element>
				<xs:element name="DocInfo" minOccurs="0">
					<xs:complexType>
						<xs:group ref="tet:DocInfoGroup" minOccurs="0"/>
