										<xs:attribute name="bitsPerComponent" use="required">
											<xs:simpleType>
												<xs:restriction base="xs:integer">
													<xs:enumeration value="-1"/>
													<xs:enumeration value="1"/>
													<xs:enumeration value="2"/>
													<xs:enumeration value="4"/>
													<xs:enumeration value="8"/>
													<xs:enumeration value="16"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:attribute>
										<xs:attribute name="colorspace" type="tet:colorSpaceId" use="optional"/>
										<xs:attribute name="height" type="xs:positiveInteger" use="required"/>
										<xs:attribute name="id" type="tet:imageId" use="required"/>
										<xs:attribute name="mask" type="tet:imageId" use="optional"/>
										<xs:attribute name="maskonly" type="xs:boolean" use="optional" default="false"/>
										<xs:attribute name="mergetype" use="optional" default="normal">
											<xs:simpleType>
												<xs:restriction base="xs:string">
													<xs:enumeration value="normal"/>
